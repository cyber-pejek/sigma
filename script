-- ====== Stare ustawienia ======
getgenv().hop = "old"
getgenv().reducelag = true
getgenv().blackscreen = false
getgenv().AutoFarm = false
getgenv().WebhookURL = "" --If not used do not write anything

-- ====== Dodajemy efekt GUI "Banana Client 2.5" ======
local CoreGui = game:GetService("CoreGui")
local TweenService = game:GetService("TweenService")

-- Usuwamy stare GUI, jeśli było
if CoreGui:FindFirstChild("BananaClientOverlay") then
    CoreGui:FindFirstChild("BananaClientOverlay"):Destroy()
end

local gui = Instance.new("ScreenGui")
gui.Name = "BananaClientOverlay"
gui.IgnoreGuiInset = true
gui.ResetOnSpawn = false
gui.Parent = CoreGui

-- Tło
local background = Instance.new("Frame")
background.Size = UDim2.new(1, 0, 1, 0)
background.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
background.BackgroundTransparency = 0.3
background.Parent = gui

-- Napis
local label = Instance.new("TextLabel")
label.Size = UDim2.new(1, 0, 1, 0)
label.BackgroundTransparency = 1
label.TextColor3 = Color3.fromRGB(255, 225, 50)
label.Font = Enum.Font.FredokaOne
label.TextScaled = true
label.TextStrokeTransparency = 0.2
label.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
label.Text = "Banana Client 2.5"
label.Parent = gui

-- Animacja pulsowania
task.spawn(function()
    while gui.Parent do
        TweenService:Create(label, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
            TextSize = 120,
            TextColor3 = Color3.fromRGB(255, 255, 100)
        }):Play()
        task.wait(1)
        TweenService:Create(label, TweenInfo.new(1, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut), {
            TextSize = 140,
            TextColor3 = Color3.fromRGB(255, 200, 50)
        }):Play()
        task.wait(1)
    end
end)

-- ====== Tutaj wciąż wczytujemy stary skrypt ======
loadstring(game:HttpGet("https://raw.githubusercontent.com/caomod2077/Script/refs/heads/main/FoxnameFarmdiamond.lua"))()
